  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "node": "concurrently -k \"npm run dev\" \"npm run watch-css\"",
    "dev": "nodemon index.js",
    "build-css": "node-sass --include-path scss scss/main.scss public/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
