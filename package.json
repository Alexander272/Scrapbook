    "connect-mongodb-session": "^2.2.0",
    "csurf": "^1.10.0",
    "cyrillic-to-translit-js": "^3.0.1",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.1",
    "googleapis": "^45.0.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.5",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
